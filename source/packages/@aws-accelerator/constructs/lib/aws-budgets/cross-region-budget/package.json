{
  "name": "@aws-accelerator/constructs-aws-budgets-cross-region-definition",
  "version": "0.0.0",
  "private": true,
  "description": "A custom resource to create a Budget Defnition in unavailable regions.",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
