{
  "name": "@aws-accelerator/utils",
  "version": "0.0.0",
  "private": true,
  "description": "Accelerator utilities",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "main": "dist/packages/@aws-accelerator/utils/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "cleanup": "tsc --build ./ --clean && rm -rf node_modules && rm -rf dist && rm -rf cdk.out",
    "cleanup:tsc": "tsc --build ./ --clean",
    "lint": "eslint --fix  --max-warnings 0 -c ../../../eslint.config.mjs 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "precommit": "eslint --max-warnings 0 -c ../../../eslint.config.mjs 'lib/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "test:unit": "vitest run --coverage --passWithNoTests",
    "test:clean": "rm -f ./test-reports/*.xml",
    "watch": "tsc -w"
  },
  "dependencies": {
    "exponential-backoff": "3.1.1"
  },
  "devDependencies": {
    "@types/lodash": "4.17.0",
    "@types/node": "18.19.50",    
    "@aws-cdk/cloudformation-diff": "2.158.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-license-header": "0.8.0",
    "eslint-plugin-prettier": "5.5.4",
    "glob": "10.3.10",
    "prettier": "3.6.2",
    "vitest": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "5.8.3",
    "winston": "3.8.2"
  },
  "nohoist": [
    "**/exponential-backoff",
    "**/exponential-backoff/**"
  ]
}
